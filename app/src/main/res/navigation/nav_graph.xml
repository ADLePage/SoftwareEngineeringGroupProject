<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.baads.mainFiles.MainPage"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_mainpage">

        <action
            android:id="@+id/action_FirstFragment_to_musicPlayer"
            app:destination="@id/musicPlayer" />
        <action
            android:id="@+id/action_FirstFragment_to_soundBar"
            app:destination="@id/soundBar" />
        <action
            android:id="@+id/action_FirstFragment_to_agenda"
            app:destination="@id/agenda" />
        <action
            android:id="@+id/action_FirstFragment_to_selfCareList"
            app:destination="@id/selfCareList" />
        <action
            android:id="@+id/action_FirstFragment_to_wikiTips"
            app:destination="@id/wikiTips" />
        <action
            android:id="@+id/action_FirstFragment_to_dailyActivity2"
            app:destination="@id/dailyActivity2" />
        <action
            android:id="@+id/action_FirstFragment_to_alarmActivityReworkFragment"
            app:destination="@id/alarmActivityReworkFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_positiveAffirmationsReworkFragment"
            app:destination="@id/positiveAffirmationsReworkFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_journalPageFragmentReworked"
            app:destination="@id/journalPageFragmentReworked" />
        <action
            android:id="@+id/action_FirstFragment_to_youtubePlayerFragmentReworked"
            app:destination="@id/youtubePlayerFragmentReworked" />
    </fragment>
    <fragment
        android:id="@+id/musicPlayer"
        android:name="com.example.baads.MusicPlayer"
        android:label="MusicPlayer" />
    <activity
        android:id="@+id/soundBar"
        android:name="com.example.baads.SoundBar"
        android:label="activity_sound_bar"
        tools:layout="@layout/activity_sound_bar" />
    <activity
        android:id="@+id/agenda"
        android:name="com.example.baads.Agenda"
        android:label="activity_agenda"
        tools:layout="@layout/activity_agenda" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.baads.mainFiles.LoginFragment"
        android:label="fragment_first2"
        tools:layout="@layout/fragment_first2" >
        <action
            android:id="@+id/action_loginFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>
    <activity
        android:id="@+id/selfCareList"
        android:name="com.example.baads.SelfCareList"
        android:label="activity_self_care_list"
        tools:layout="@layout/activity_self_care_list" />
    <activity
        android:id="@+id/wikiTips"
        android:name="com.example.baads.WikiTips"
        android:label="WikiTips" />

    <activity
        android:id="@+id/dailyActivity2"
        android:name="com.example.baads.dailyActivity"
        android:label="activity_daily"
        tools:layout="@layout/activity_daily" />
    <fragment
        android:id="@+id/alarmActivityReworkFragment"
        android:name="com.example.baads.alarm.alarmActivityReworkFragment"
        android:label="alarmActivityReworkFragment" />
    <fragment
        android:id="@+id/positiveAffirmationsReworkFragment"
        android:name="com.example.baads.positiveThoughts.positiveAffirmationsReworkFragment"
        android:label="positiveAffirmationsReworkFragment" />
    <fragment
        android:id="@+id/journalPageFragmentReworked"
        android:name="com.example.baads.journalPage.journalPageFragmentReworked"
        android:label="journalPageFragmentReworked" />
    <fragment
        android:id="@+id/youtubePlayerFragmentReworked"
        android:name="com.example.baads.youtubePlayer.youtubePlayerFragmentReworked"
        android:label="youtubePlayerFragmentReworked" />

</navigation>

